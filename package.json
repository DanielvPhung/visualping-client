{
  "name": "visualping-client",
  "version": "1.0.2",
  "description": "Visualping API wrapper for Node.js with TypeScript support",
  "homepage": "https://github.com/DanielvPhung/visualping-client#readme",
  "bugs": {
    "url": "https://github.com/DanielvPhung/visualping-client/issues"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/DanielvPhung/visualping-client.git"
  },
  "license": "MIT",
  "author": "Daniel Phung",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "tsc -p tsconfig.json",
    "typecheck:tests": "tsc -p tsconfig.test.json",
    "test": "npm run build && node --import tsx --test __tests__",
    "test:dev": "node --import tsx --test __tests__/**/*.test.ts",
    "lint": "eslint src/**/*.ts",
    "format": "prettier --write \"src/**/*.ts\"",
    "format:check": "prettier --check \"src/**/*.ts\"",
    "prepublishOnly": "npm run lint && npm run typecheck:tests && npm test"
  },
  "keywords": [
    "visualping",
    "visual-ping",
    "website-monitoring",
    "change-detection",
    "api-wrapper",
    "typescript"
  ],
  "engines": {
    "node": ">=20.0.0"
  },
  "devDependencies": {
    "@types/node": "^20.19.27",
    "@typescript-eslint/eslint-plugin": "^8.50.1",
    "@typescript-eslint/parser": "^8.50.1",
    "eslint": "^9.39.2",
    "prettier": "^3.7.4",
    "tsx": "^4.21.0",
    "typescript": "^5.0.0",
    "typescript-eslint": "^8.50.1"
  }
}
